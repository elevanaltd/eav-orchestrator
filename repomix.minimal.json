{
  "output": {
    "filePath": ".coord/context/minimal-context.xml",
    "style": "xml",
    "parsableStyle": false,
    "compress": false,
    "headerText": "EAV Orchestrator Minimal Context for Session Initialization",
    "fileSummary": true,
    "directoryStructure": true,
    "files": true,
    "removeComments": true,
    "removeEmptyLines": true,
    "topFilesLength": 10,
    "showLineNumbers": false,
    "copyToClipboard": false,
    "includeEmptyDirectories": false,
    "git": {
      "sortByChanges": true,
      "sortByChangesMaxCommits": 20,
      "includeDiffs": false
    }
  },
  "include": [
    "README.md",
    "package.json",
    "src/main.tsx",
    "src/App.tsx",
    "src/types/scriptComponent.ts",
    "src/lib/collaboration/YjsSupabaseProvider.ts",
    "src/lib/collaboration/custom-supabase-provider.ts",
    "docs/105-DOC-EAV-ORCHESTRATOR-D1-NORTH-STAR.md",
    "supabase/migrations/*.sql"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "tests/**",
      "docs/2*.md",
      "docs/1[1-9]*.md",
      "**/*.test.ts",
      "**/*.spec.ts",
      "coverage/**",
      "dist/**",
      "build/**",
      ".coord/context/*.xml"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}
